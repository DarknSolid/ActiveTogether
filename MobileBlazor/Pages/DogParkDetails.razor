@attribute [Route(RoutingConstants.DOG_PARK_DETAILS + "{id:int}")]
@using MobileBlazor.Extensions
@using static EntityLib.Entities.Enums
@inject NavigationManager NavigationManager

<RazorLib.Pages.DogParkDetails 
    DogParkId=Id 
    OnCreateReviewPressed="(string dogParkName, bool isReviewUpdate) => 
        NavigationManager.NavigateToReviewCreate(Id, FacilityType.DogPark, dogParkName, isReviewUpdate)"
/>

@code {
    [Parameter]
    public int Id { get; set; }
}